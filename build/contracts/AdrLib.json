  "source": "pragma solidity ^0.4.4;\n\nlibrary AdrLib {\n  /*function AddInList(address[] storage adrL, address) returns (uint ind) {\n    \n  }*/\n\n  function assertInside(address[] storage adrL, address adr) public view returns (bool) {\n    for (uint i = 0; i < adrL.length; i++) {\n      if (adr == adrL[i]) {\n        return true;\n      }\n    }\n    return false;\n  }\n\n}\n",
